# Private key for deployment (DO NOT COMMIT TO VERSION CONTROL)
PRIVATE_KEY=your-private-key-here

# FDC Attestor Configuration
# Local FDC attestor REST API endpoint (default: http://localhost:8080)
FDC_ATTESTOR_URL=http://localhost:8080

# API key for authenticating with FDC attestor
# Must match the API key in your FDC client config (userConfig.toml)
FDC_API_KEY=your-secure-api-key-change-this

# Verifier server configuration (should match verifier-server/.env)
VERIFIER_URL=http://localhost:3001
VERIFIER_API_KEY=your-secure-api-key-change-this

# Network RPC URLs (optional, uses public endpoints by default)
COSTON2_RPC_URL=https://coston2-api.flare.network/ext/C/rpc
FLARE_RPC_URL=https://flare-api.flare.network/ext/C/rpc
